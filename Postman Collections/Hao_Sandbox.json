        {
          "name": "CFB Date Fetch",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "dd93c25b-9880-4227-8c97-6885295b8d0f",
                "exec": [
                  "\r",
                  "var jsonData = JSON.parse(responseBody);\r",
                  "\r",
                  "const moment = require('moment');\r",
                  "pm.environment.set(\"sdS6Timestamp\", moment().format(\"YYYYMMDD\"));\r",
                  "\r",
                  "//postman.setEnvironmentVariable(\"sdS6Timestamp\", jsonData.currentSelectionId);\r",
                  "\r",
                  "pm.test(\"MLB Date - Status code is 200\", function () {\r",
                  "    pm.response.to.have.status(200);\r",
                  "});"
                ],
